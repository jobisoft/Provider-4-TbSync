<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">
        <em:id>ews4tbsync@jobisoft.de</em:id>
        <em:type>2</em:type> <!-- type=extension --> 
        <em:bootstrap>true</em:bootstrap>
        <em:unpack>false</em:unpack>
        <em:multiprocessCompatible>true</em:multiprocessCompatible>
        <em:name>Provider for Exchange WebServices</em:name>
        <em:version>0.6</em:version>
        <em:description>Add sync support for Exchange WebServices accounts to TbSync</em:description>
        <em:translator>John Bieling (en-US)</em:translator>
        <em:translator>Alexey Sinitsyn (ru)</em:translator>
        <em:creator>John Bieling</em:creator>
        <em:homepageURL>https://github.com/jobisoft/EWS-4-TbSync</em:homepageURL>
        <em:iconURL>chrome://ews4tbsync/skin/exchange-addrbook.png</em:iconURL>

        <em:targetApplication>
          <!-- Thunderbird -->
          <Description>
            <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
            <em:minVersion>60.0</em:minVersion>
            <em:maxVersion>61.*</em:maxVersion>
          </Description>
        </em:targetApplication>

         <em:localized>  <!-- Russian -->
          <Description>
            <em:locale>ru</em:locale>
            <em:name>Провайдер CalDAV &amp; CardDAV</em:name>
            <em:description>Добавляет в TbSync поддержку базирующегося на http/https протокола синхронизации для учетных записей CalDAV &amp; CardDAV (контакты, задачи и календари)</em:description>
            <em:translator>John Bieling (en-US)</em:translator>
            <em:translator>Alexey Sinitsyn (ru)</em:translator> 
            <em:creator>John Bieling</em:creator>
          </Description>
         </em:localized>
         
    </Description>
</RDF>
